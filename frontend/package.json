{
  "name": "valentine-wall",
  "version": "0.0.0",
  "scripts": {
    "dev": "node server",
    "build": "npm run build:client && npm run build:server",
    "build:noExternal": "npm run build:client && npm run build:server:noExternal",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/main-server.js",
    "build:server:noExternal": "vite build --config vite.config.noexternal.ts --outDir dist/server --ssr src/main-server.js",
    "generate": "vite build --ssrManifest --outDir dist/static && npm run build:server && node prerender",
    "serve": "node server",
    "preview": "vite preview"
  },
  "dependencies": {
    "@iconify-json/twemoji": "^1.0.2",
    "@polka/send-type": "^0.5.2",
    "@vueuse/head": "^0.7.5",
    "compression": "^1.7.4",
    "daisyui": "^1.22.0",
    "dayjs": "^1.10.7",
    "firebase": "^9.6.3",
    "masonry-layout": "^4.2.2",
    "node-fetch": "^3.2.0",
    "notiwind": "^1.2.4",
    "polka": "^0.5.2",
    "sirv": "^2.0.2",
    "vite-plugin-compress": "^2.1.0",
    "vite-plugin-windicss": "^1.6.2",
    "vue": "^3.2.25",
    "vue-head": "MKGaru/vue-head#vue3",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "windicss": "^3.4.2"
  },
  "devDependencies": {
    "@iconify-json/logos": "^1.0.4",
    "@iconify-json/uil": "^1.0.2",
    "@types/compression": "^1.7.2",
    "@types/facebook-js-sdk": "^3.3.5",
    "@types/masonry-layout": "^4.2.4",
    "@types/polka": "^0.5.4",
    "@vitejs/plugin-vue": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.13",
    "typescript": "^4.4.4",
    "unplugin-icons": "^0.13.0",
    "vite": "^2.7.2",
    "vite-plugin-checker": "^0.3.4",
    "vite-plugin-iso-import": "^0.1.3",
    "vite-plugin-markdown": "^2.0.2",
    "vue-tsc": "^0.29.8"
  }
}
