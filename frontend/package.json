{
  "name": "valentine-wall",
  "version": "0.0.0",
  "scripts": {
    "dev": "node server",
    "build": "npm run build:client && npm run build:server",
    "build:noExternal": "npm run build:client && npm run build:server:noExternal",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --config vite.config.server.ts --outDir dist/server --ssr src/main-server.js",
    "build:server:noExternal": "vite build --config vite.config.noexternal.ts --outDir dist/server --ssr src/main-server.js",
    "generate": "vite build --ssrManifest --outDir dist/static && npm run build:server && node prerender",
    "serve": "node server",
    "preview": "vite preview"
  },
  "dependencies": {
    "@iconify-json/twemoji": "^1.0.2",
    "@polka/send-type": "^0.5.2",
    "@tanstack/vue-query": "^4.24.4",
    "@vueuse/head": "^0.7.5",
    "compression": "^1.7.4",
    "daisyui": "^2.49.0",
    "dayjs": "^1.10.7",
    "event-source-polyfill": "^1.0.25",
    "firebase": "^9.17.1",
    "floating-vue": "^2.0.0-beta.20",
    "masonry-layout": "^4.2.2",
    "morgan": "^1.10.0",
    "notiwind": "nedpals/notiwind",
    "pocketbase": "^0.10.1",
    "polka": "^0.5.2",
    "sharp": "^0.31.3",
    "sirv": "^2.0.2",
    "undici": "^4.13.0",
    "vite-plugin-favicons-inject": "^2.2.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@iconify-json/logos": "^1.0.4",
    "@iconify-json/uil": "^1.0.2",
    "@tailwindcss/typography": "^0.5.2",
    "@types/compression": "^1.7.2",
    "@types/event-source-polyfill": "^1.0.0",
    "@types/facebook-js-sdk": "^3.3.5",
    "@types/masonry-layout": "^4.2.4",
    "@types/morgan": "^1.9.3",
    "@types/polka": "^0.5.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "unplugin-icons": "^0.13.0",
    "vite": "^4.0.0",
    "vite-plugin-checker": "^0.5.5",
    "vite-plugin-iso-import": "^1.0.0",
    "vite-plugin-markdown": "^2.1.0",
    "vue-tsc": "^0.29.8"
  }
}
